Cell[CellGroupData[{

Cell["Determining the Exact Solution ", "Subsection"],

Cell["\<\
This is a differential equation which can be solved exactly, so we will do \
that and then compare the approximations with the exact results as we \
proceed.\
\>", "Text"],

Cell[BoxData[{\(Clear[y, t]\), "\[IndentingNewLine]", 
    RowBox[{\(f[t_, y_]\), ":=", 
      StyleBox[\(\(-100\) y\  + \ 100  t + 1\),
        FontColor->RGBColor[1, 0, 0]]}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"t0", "=", 
        StyleBox["0",
          FontColor->RGBColor[1, 0, 0]]}], ";"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"y0", "=", 
        StyleBox["1",
          FontColor->RGBColor[1, 0, 0]]}], 
      ";"}], "\[IndentingNewLine]", \(desol = 
      DSolve[{\(y'\)[t] \[Equal] f[t, y[t]], y[t0] \[Equal] y0}, y[t], t] // 
        Simplify\), "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"exactsoln", "=", 
        RowBox[{"Table", "[", 
          RowBox[{\({t, \((y[t] /. desol)\)[\([1]\)]}\), ",", 
            RowBox[{"{", 
              RowBox[{"t", ",", "t0", ",", 
                StyleBox[".1",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox[",",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox[".05",
                  FontColor->RGBColor[1, 0, 0]]}], "}"}]}], "]"}]}], 
      ";"}], "\[IndentingNewLine]", \(exactsoln // TableForm\)}], "Input"]
}, Closed]],

