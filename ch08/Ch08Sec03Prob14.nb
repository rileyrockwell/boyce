      \(TraditionalForm\`y\^2\)]]
}], "Section",
  PageWidth->WindowWidth],

Cell[CellGroupData[{

Cell["Drawing the Direction Field ", "Subsection"],

Cell["\<\
We will load the graphics package and plot the direction field as we did in \
earlier chapters. We need to first load a graphics package.\
\>", "Text"],

Cell[BoxData[
    \(<< Graphics`PlotField`\)], "Input"],

Cell[BoxData[{\(Clear[y, t, yprime]\), "\[IndentingNewLine]", 
    RowBox[{\(f[t_, y_]\), ":=", 
      RowBox[{
        StyleBox[\(t\^2\),
          FontColor->RGBColor[1, 0, 0]], 
        StyleBox[" ",
          FontColor->RGBColor[1, 0, 0]], 
        StyleBox["+",
          FontColor->RGBColor[1, 0, 0]], 
        StyleBox[" ",
          FontColor->RGBColor[1, 0, 0]], 
        StyleBox[\(y\^2\),
          FontColor->RGBColor[1, 0, 0]]}]}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"tmin", "=", 
        StyleBox["0",
          FontColor->RGBColor[1, 0, 0]]}], ";", " ", 
      RowBox[{"tmax", "=", 
        StyleBox["1",
          FontColor->RGBColor[1, 0, 0]]}], ";", " ", 
      RowBox[{"ymin", "=", 
        StyleBox["0",
          FontColor->RGBColor[1, 0, 0]]}], ";", " ", 
      RowBox[{"ymax", "=", 
        StyleBox["51",
          FontColor->RGBColor[1, 0, 0]]}], 
      ";"}], "\[IndentingNewLine]", \(pd = 
        PlotVectorField[{1, f[t, y]}, {t, tmin, tmax}, {y, ymin, ymax}, \ 
          Axes \[Rule] True, AxesLabel \[Rule] {t, y}, 
          ScaleFactor \[Rule] 15, AspectRatio \[Rule] 1];\)}], "Input"]
}, Closed]],

Cell[CellGroupData[{

Cell["Using the Runge-Kutta Method to Approximate the Solution", "Subsection"],

Cell["First we will try a step size of h = 0.05.", "Text"],

Cell[BoxData[{\(Clear[f, t, y, new, solnslist1, 
      solnslist01]\), "\[IndentingNewLine]", 
    RowBox[{\(f[t_, y_]\), ":=", 
      RowBox[{
        StyleBox[\(t\^2\),
          FontColor->RGBColor[1, 0, 0]], 
        StyleBox[" ",
          FontColor->RGBColor[1, 0, 0]], 
        StyleBox["+",
          FontColor->RGBColor[1, 0, 0]], 
        StyleBox[" ",
          FontColor->RGBColor[1, 0, 0]], 
        StyleBox[\(y\^2\),
          FontColor->RGBColor[1, 0, 0]]}]}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"t0", "=", 
        StyleBox["0",
          FontColor->RGBColor[1, 0, 0]]}], ";"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"y0", "=", 
        StyleBox["1",
          FontColor->RGBColor[1, 0, 0]]}], ";"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"h", "=", 
        StyleBox[".05",
          FontColor->RGBColor[1, 0, 0]]}], 
      ";"}], "\[IndentingNewLine]", \(solnslist1 = {{t0, 
            y0}};\), "\[IndentingNewLine]", 
    RowBox[{"Do", "[", 
      RowBox[{\({k1 = f[t0, y0]\ , k2 = f[t0 + 1/2  h, y0 + 1/2\ h\ k1], 
          k3 = f[t0 + 1/2  h, y0 + 1/2\ h\ k2], k4 = f[t0 + h, y0 + h\ k3], 
          new = y0 + h/6\ \((k1 + 2  k2 + 2  k3 + k4)\), t0 = t0 + h, 
          y0 = new, AppendTo[solnslist1, {t0, y0}]}\), ",", 
        RowBox[{"{", 
          StyleBox["20",
            FontColor->RGBColor[1, 0, 0]], "}"}]}], 
      "]"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"compare1", "=", 
        RowBox[{"Table", "[", 
          RowBox[{\(solnslist1[\([i]\)]\), ",", 
            RowBox[{"{", 
              RowBox[{"i", ",", 
                StyleBox["1",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox[",",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox["21",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox[",",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox["1",
                  FontColor->RGBColor[1, 0, 0]]}], "}"}]}], "]"}]}], 
      ";"}], "\[IndentingNewLine]", \(compare1 // TableForm\)}], "Input"],

Cell["\<\
The huge jump in the value of y as t goes from 0.95 to 1 indicates that y is \
increasing very rapidly there. Remember that the rate of change of y is the \
sum of the squares of t and y.
Let's try a smaller value of h.\
\>", "Text"],

Cell[BoxData[{\(Clear[f, t, y, new, solnslist1, 
      solnslist01]\), "\[IndentingNewLine]", 
    RowBox[{\(f[t_, y_]\), ":=", 
      RowBox[{
        StyleBox[\(t\^2\),
          FontColor->RGBColor[1, 0, 0]], 
        StyleBox[" ",
          FontColor->RGBColor[1, 0, 0]], 
        StyleBox["+",
          FontColor->RGBColor[1, 0, 0]], 
        StyleBox[" ",
          FontColor->RGBColor[1, 0, 0]], 
        StyleBox[\(y\^2\),
          FontColor->RGBColor[1, 0, 0]]}]}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"t0", "=", 
        StyleBox["0",
          FontColor->RGBColor[1, 0, 0]]}], ";"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"y0", "=", 
        StyleBox["1",
          FontColor->RGBColor[1, 0, 0]]}], ";"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"h", "=", 
        StyleBox[".01",
          FontColor->RGBColor[1, 0, 0]]}], 
      ";"}], "\[IndentingNewLine]", \(solnslist1 = {{t0, 
            y0}};\), "\[IndentingNewLine]", 
    RowBox[{"Do", "[", 
      RowBox[{\({k1 = f[t0, y0]\ , k2 = f[t0 + 1/2  h, y0 + 1/2\ h\ k1], 
          k3 = f[t0 + 1/2  h, y0 + 1/2\ h\ k2], k4 = f[t0 + h, y0 + h\ k3], 
          new = y0 + h/6\ \((k1 + 2  k2 + 2  k3 + k4)\), t0 = t0 + h, 
          y0 = new, AppendTo[solnslist1, {t0, y0}]}\), ",", 
        RowBox[{"{", 
          StyleBox["100",
            FontColor->RGBColor[1, 0, 0]], "}"}]}], 
      "]"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"compare2", "=", 
        RowBox[{"Table", "[", 
          RowBox[{\(solnslist1[\([i]\)]\), ",", 
            RowBox[{"{", 
              RowBox[{"i", ",", 
                StyleBox["1",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox[",",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox["101",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox[",",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox["5",
                  FontColor->RGBColor[1, 0, 0]]}], "}"}]}], "]"}]}], 
      ";"}], "\[IndentingNewLine]", \(compare2 // TableForm\)}], "Input"],

Cell["\<\
The results are much the same for the smaller values of t, but an even worse \
problem seems to be occurring as t approaches 1. We will decrease the step \
size and see what happens.\
\>", "Text"],

Cell[BoxData[{\(Clear[f, t, y, new, solnslist1, 
      solnslist01]\), "\[IndentingNewLine]", 
    RowBox[{\(f[t_, y_]\), ":=", 
      RowBox[{
        StyleBox[\(t\^2\),
          FontColor->RGBColor[1, 0, 0]], 
        StyleBox[" ",
          FontColor->RGBColor[1, 0, 0]], 
        StyleBox["+",
          FontColor->RGBColor[1, 0, 0]], 
        StyleBox[" ",
          FontColor->RGBColor[1, 0, 0]], 
        StyleBox[\(y\^2\),
          FontColor->RGBColor[1, 0, 0]]}]}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"t0", "=", 
        StyleBox["0",
          FontColor->RGBColor[1, 0, 0]]}], ";"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"y0", "=", 
        StyleBox["1",
          FontColor->RGBColor[1, 0, 0]]}], ";"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"h", "=", 
        StyleBox[".005",
          FontColor->RGBColor[1, 0, 0]]}], 
      ";"}], "\[IndentingNewLine]", \(solnslist1 = {{t0, 
            y0}};\), "\[IndentingNewLine]", 
    RowBox[{"Do", "[", 
      RowBox[{\({k1 = f[t0, y0]\ , k2 = f[t0 + 1/2  h, y0 + 1/2\ h\ k1], 
          k3 = f[t0 + 1/2  h, y0 + 1/2\ h\ k2], k4 = f[t0 + h, y0 + h\ k3], 
          new = y0 + h/6\ \((k1 + 2  k2 + 2  k3 + k4)\), t0 = t0 + h, 
          y0 = new, AppendTo[solnslist1, {t0, y0}]}\), ",", 
        RowBox[{"{", 
          StyleBox["200",
            FontColor->RGBColor[1, 0, 0]], "}"}]}], 
      "]"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"compare3", "=", 
        RowBox[{"Table", "[", 
          RowBox[{\(solnslist1[\([i]\)]\), ",", 
            RowBox[{"{", 
              RowBox[{"i", ",", 
                StyleBox["1",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox[",",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox["201",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox[",",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox["10",
                  FontColor->RGBColor[1, 0, 0]]}], "}"}]}], "]"}]}], 
      ";"}], "\[IndentingNewLine]", \(compare3 // TableForm\)}], "Input"],

Cell["\<\
Note how the values of the function are stabilizing for t = 0.95 or less, but \
the problem continues to grow as t approaches 1. We will cut the step size in \
half.\
\>", "Text"],

Cell[BoxData[{\(Clear[f, t, y, new, solnslist1, 
      solnslist01]\), "\[IndentingNewLine]", 
    RowBox[{\(f[t_, y_]\), ":=", 
      RowBox[{
        StyleBox[\(t\^2\),
          FontColor->RGBColor[1, 0, 0]], 
        StyleBox[" ",
          FontColor->RGBColor[1, 0, 0]], 
        StyleBox["+",
          FontColor->RGBColor[1, 0, 0]], 
        StyleBox[" ",
          FontColor->RGBColor[1, 0, 0]], 
        StyleBox[\(y\^2\),
          FontColor->RGBColor[1, 0, 0]]}]}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"t0", "=", 
        StyleBox["0",
          FontColor->RGBColor[1, 0, 0]]}], ";"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"y0", "=", 
        StyleBox["1",
          FontColor->RGBColor[1, 0, 0]]}], ";"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"h", "=", 
        StyleBox[".0025",
          FontColor->RGBColor[1, 0, 0]]}], 
      ";"}], "\[IndentingNewLine]", \(solnslist1 = {{t0, 
            y0}};\), "\[IndentingNewLine]", 
    RowBox[{"Do", "[", 
      RowBox[{\({k1 = f[t0, y0]\ , k2 = f[t0 + 1/2  h, y0 + 1/2\ h\ k1], 
          k3 = f[t0 + 1/2  h, y0 + 1/2\ h\ k2], k4 = f[t0 + h, y0 + h\ k3], 
          new = y0 + h/6\ \((k1 + 2  k2 + 2  k3 + k4)\), t0 = t0 + h, 
          y0 = new, AppendTo[solnslist1, {t0, y0}]}\), ",", 
        RowBox[{"{", 
          StyleBox["400",
            FontColor->RGBColor[1, 0, 0]], "}"}]}], 
      "]"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"compare4", "=", 
        RowBox[{"Table", "[", 
          RowBox[{\(solnslist1[\([i]\)]\), ",", 
            RowBox[{"{", 
              RowBox[{"i", ",", 
                StyleBox["1",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox[",",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox["401",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox[",",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox["20",
                  FontColor->RGBColor[1, 0, 0]]}], "}"}]}], "]"}]}], 
      ";"}], "\[IndentingNewLine]", \(compare4 // TableForm\)}], "Input"],

Cell["\<\
Again the values seem to have stabilized except at t = 1. We will stop here \
and plot this set of data (excluding the Overflow[].\
\>", "Text"],

Cell[BoxData[
    \(lp = 
      ListPlot[compare4, PlotRange \[Rule] {0, 51}, 
        PlotStyle \[Rule] PointSize[ .02]]\)], "Input"]
}, Closed]],

Cell[CellGroupData[{

Cell[TextData[{
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  " Numerical Solution"
}], "Subsection"],

Cell[TextData[{
  "We cannot find an analytical solution to this differential equation, but \
we can attempt to find a numerical solution using the ",
  StyleBox["DSolve",
    FontWeight->"Bold"],
  " command."
}], "Text"],

Cell[BoxData[{\(Clear[f, t, y]\), "\[IndentingNewLine]", 
    RowBox[{\(f[t_, y_]\), ":=", 
      RowBox[{
        StyleBox[\(t\^2\),
          FontColor->RGBColor[1, 0, 0]], 
        StyleBox[" ",
          FontColor->RGBColor[1, 0, 0]], 
        StyleBox["+",
          FontColor->RGBColor[1, 0, 0]], 
        StyleBox[" ",
          FontColor->RGBColor[1, 0, 0]], 
        StyleBox[\(y\^2\),
          FontColor->RGBColor[1, 0, 0]]}]}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"t0", "=", 
        StyleBox["0",
          FontColor->RGBColor[1, 0, 0]]}], ";"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"y0", "=", 
        StyleBox["1",
          FontColor->RGBColor[1, 0, 0]]}], 
      ";"}], "\[IndentingNewLine]", \(numsol = 
      NDSolve[{\(y'\)[t] \[Equal] f[t, y[t]], y[t0] \[Equal] y0}, 
        y[t], {t, 0, 1}]\)}], "Input"],

Cell["\<\
This error message indicates that the numerical method failed at around t = \
0.96981, so we are unable to reach t = 1. We can look at the values \
represented by this numerical solution.\
\>", "Text"],

Cell[BoxData[{
    \(\(solntable = 
        Table[{t, numsol[\([1, 1, 2]\)]}, {t, 
            0,  .95,  .05}];\)\), "\[IndentingNewLine]", 
    \(solntable // TableForm\), "\[IndentingNewLine]", 
    \(\(plsoln = 
        Plot[y[t] /. numsol, {t, 0,  .969}, PlotRange \[Rule] {0, 51}, 
          PlotStyle \[Rule] {RGBColor[1, 0, 0]}];\)\)}], "Input"]
}, Closed]],

Cell[CellGroupData[{

Cell["Comparison", "Subsection"],

Cell["\<\
We can look at this plot with the results from the Runge-Kutta method and \
also superimpose these onto our direction field.\
\>", "Text"],

Cell[BoxData[{
    \(Show[plsoln, lp]\), "\[IndentingNewLine]", 
    \(\(Show[plsoln, pd, lp, PlotRange \[Rule] {0, 51}];\)\)}], "Input"],

Cell["\<\
The steepness of the graphs indicate the problem that is occurring as t \
approaches 1. Now, let us look at a numerical comparison.\
\>", "Text"],

Cell[BoxData[{
    \(\(comparisons = 
        Table[{solntable[\([i, 1]\)], solntable[\([i, 2]\)], 
            compare4[\([i, 2]\)], compare3[\([i, 2]\)]}, {i, 1, 
            20}];\)\), "\n", 
    \(TableForm[comparisons, 
      TableHeadings \[Rule] {None, {"\<t\>", \ "\<almost exact\>", \ \
"\<RungeKutta.0025\>", "\<RungeKutta.005\>"}}]\)}], "Input"]
}, Closed]]
}, Open  ]]
}, Open  ]]
},
