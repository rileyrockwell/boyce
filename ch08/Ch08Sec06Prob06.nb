Notebook[{

Cell[CellGroupData[{
Cell["Chapter 8  -  Section 8.6", "Title",
  PageWidth->WindowWidth],

Cell[TextData[{
  "I",
  StyleBox["n this ",
    FontWeight->"Bold"],
  StyleBox["Mathematica",
    FontWeight->"Bold",
    FontSlant->"Italic"],
  StyleBox[" notebook, you will use two numerical methods to approximate the \
solution to a system of differential equations with specified initial \
conditions. The results will be compared to a more exact solution.",
    FontWeight->"Bold"],
  "\n\n",
  StyleBox["In most cases, to adapt this code to other problems, you will \
have to change only the input shown in red. To open a cell grouping, \
double-click on the right-hand side of the double cell marker.",
    FontSize->14]
}], "Subsubtitle",
  PageWidth->WindowWidth],

Cell[CellGroupData[{

Cell[TextData[{
  "Problem 6:   x' =  ",
  Cell[BoxData[
      \(TraditionalForm\`\[ExponentialE]\^\(\(-x\) + y\)\)]],
  "- Cos(x)\n\t         y' =  Sin(x - 3y)"
}], "Section",
  PageWidth->WindowWidth],

Cell[CellGroupData[{

Cell["Determining the Exact Solution ", "Subsection"],

Cell[TextData[{
  "This is a system of differential equations which cannot be solved by the \
traditional methods, so we will use the built-in ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  " numerical solver ",
  StyleBox["NDSolve",
    FontWeight->"Bold"],
  " and then compare the approximations with the results obtained by this \
more sophisticated method.."
}], "Text"],

Cell[BoxData[{\(Clear[x, y, t]\), "\[IndentingNewLine]", 
    RowBox[{\(f[x_, y_]\), ":=", 
      StyleBox[\(\[ExponentialE]\^\(\(-x\) + y\) - Cos[x]\),
        FontColor->RGBColor[1, 0, 0]]}], "\[IndentingNewLine]", 
    RowBox[{\(g[x_, y_]\), ":=", 
      StyleBox[\(Sin[x - 3  y]\),
        FontColor->RGBColor[1, 0, 0]]}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"t0", "=", 
        StyleBox["0",
          FontColor->RGBColor[1, 0, 0]]}], ";"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"x0", "=", 
        StyleBox["1",
          FontColor->RGBColor[1, 0, 0]]}], ";"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"y0", "=", 
        StyleBox["2",
          FontColor->RGBColor[1, 0, 0]]}], 
      ";"}], "\[IndentingNewLine]", \(desol = 
      NDSolve[{\(x'\)[t] \[Equal] f[x[t], y[t]], \(y'\)[t] \[Equal] 
              g[x[t], y[t]], x[t0] \[Equal] x0, y[t0] \[Equal] y0}, {x[t], 
            y[t]}, {t, 0, 1}] // Simplify\), "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"tvalues", "=", 
        RowBox[{"Table", "[", 
          RowBox[{"t", ",", 
            RowBox[{"{", 
              RowBox[{"t", ",", "t0", ",", 
                StyleBox["1",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox[",",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox[".2",
                  FontColor->RGBColor[1, 0, 0]]}], "}"}]}], "]"}]}], 
      ";"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"exactsoln", "=", 
        RowBox[{"Table", "[", 
          
          RowBox[{\({t, \((x[t] /. desol)\)[\([1]\)]\ , \((y[t] /. 
                    desol)\)[\([1]\)]}\), ",", 
            RowBox[{"{", 
              RowBox[{"t", ",", "t0", ",", 
                StyleBox["1",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox[",",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox[".2",
                  FontColor->RGBColor[1, 0, 0]]}], "}"}]}], "]"}]}], 
      ";"}], "\[IndentingNewLine]", \(TableForm[exactsoln, 
      TableHeadings \[Rule] {None, {"\<t\>", "\<x\>", "\<y\>"}}]\)}], "Input"]
}, Closed]],

Cell[CellGroupData[{

Cell["Applying Euler's Method with h = 0.1", "Subsection"],

Cell["\<\
We will begin by defining our function and detailing the steps that are to be \
followed in applying Euler's method. Note that here, we are applying the \
Euler formula to both x and y.\
\>", "Text"],

Cell[BoxData[{\(Clear[f, t, y, newx, newy, 
      solnslist1]\), "\[IndentingNewLine]", 
    RowBox[{\(f[x_, y_]\), ":=", 
      StyleBox[\(\[ExponentialE]\^\(\(-x\) + y\) - Cos[x]\),
        FontColor->RGBColor[1, 0, 0]]}], "\[IndentingNewLine]", 
    RowBox[{\(g[x_, y_]\), ":=", 
      StyleBox[\(Sin[x - 3  y]\),
        FontColor->RGBColor[1, 0, 0]]}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"t0", "=", 
        StyleBox["0",
          FontColor->RGBColor[1, 0, 0]]}], ";"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"x0", "=", 
        StyleBox["1",
          FontColor->RGBColor[1, 0, 0]]}], ";"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"y0", "=", 
        StyleBox["2",
          FontColor->RGBColor[1, 0, 0]]}], ";"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"h", "=", 
        StyleBox[".1",
          FontColor->RGBColor[1, 0, 0]]}], 
      ";"}], "\[IndentingNewLine]", \(solnslist1 = {{t0, x0, 
            y0}};\), "\[IndentingNewLine]", 
    RowBox[{"Do", "[", 
      RowBox[{\({newx = x0 + f[x0, y0]\ h, newy = y0 + g[x0, y0]\ h, 
          t0 = t0 + h, x0 = newx, y0 = newy, 
          AppendTo[solnslist1, {t0, x0, y0}]}\), ",", 
        RowBox[{"{", 
          StyleBox["10",
            FontColor->RGBColor[1, 0, 0]], "}"}]}], 
      "]"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"compare1", "=", 
        RowBox[{"Table", "[", 
          RowBox[{\({solnslist1[\([i]\)]}\), ",", 
            RowBox[{"{", 
              RowBox[{"i", ",", 
                StyleBox["1",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox[",",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox["11",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox[",",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox["2",
                  FontColor->RGBColor[1, 0, 0]]}], "}"}]}], "]"}]}], 
      ";"}], "\[IndentingNewLine]", \(fulllist1 = 
        Table[{exactsoln[\([i, 1]\)], exactsoln[\([i, 2]\)], 
            compare1[\([i, 1, 2]\)], exactsoln[\([i, 3]\)], 
            compare1[\([i, 1, 3]\)]}, {i, 1, 6}];\), "\[IndentingNewLine]", 
    RowBox[{\(TableForm[fulllist1, 
        TableHeadings \[Rule] {None, {"\<t\>", "\<x\>", "\<Euler x\>", \
"\<y\>", "\<Euler y\>"}}]\), "  "}]}], "Input"],

Cell["These results are not bad for such a basic method.", "Text"]
}, Closed]],

Cell[CellGroupData[{

Cell["Applying the Runge-Kutta Method with h = 0.2", "Subsection"],

Cell["\<\
Applying the Runge-Kutta method to this system of equations, we need to \
compute the set of k's for both x and y. The formulas follow the same pattern \
as those used for a single dependent variable. You may ignore warnings about \
possible spelling errors here.\
\>", "Text"],

Cell[BoxData[{\(Clear[f, t, y, newx, newy, 
      solnslist2]\), "\[IndentingNewLine]", 
    RowBox[{\(f[x_, y_]\), ":=", 
      StyleBox[\(\[ExponentialE]\^\(\(-x\) + y\) - Cos[x]\),
        FontColor->RGBColor[1, 0, 0]]}], "\[IndentingNewLine]", 
    RowBox[{\(g[x_, y_]\), ":=", 
      StyleBox[\(Sin[x - 3  y]\),
        FontColor->RGBColor[1, 0, 0]]}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"t0", "=", 
        StyleBox["0",
          FontColor->RGBColor[1, 0, 0]]}], ";"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"x0", "=", 
        StyleBox["1",
          FontColor->RGBColor[1, 0, 0]]}], ";"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"y0", "=", 
        StyleBox["2",
          FontColor->RGBColor[1, 0, 0]]}], ";"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"h", "=", 
        StyleBox[".2",
          FontColor->RGBColor[1, 0, 0]]}], 
      ";"}], "\[IndentingNewLine]", \(solnslist2 = {{t0, x0, 
            y0}};\), "\[IndentingNewLine]", 
    RowBox[{"Do", "[", 
      RowBox[{\({k1x = f[x0, y0]\ , k1y = g[x0, y0], 
          k2x = f[x0 + 1/2  h\ k1x, y0 + 1/2\ h\ k1y], 
          k2y = g[x0 + 1/2  h\ k1x, y0 + 1/2\ h\ k1y], 
          k3x = f[x0 + 1/2  h\ k2x, y0 + 1/2\ h\ k2y], 
          k3y = g[x0 + 1/2  h\ k2x, y0 + 1/2\ h\ k2y], 
          k4x = f[x0 + h\ k3x, y0 + h\ k3y], 
          k4y = g[x0 + h\ k3x, y0 + h\ k3y], 
          newx = x0 + h/6\ \((k1x + 2  k2x + 2  k3x + k4x)\), 
          newy = y0 + h/6\ \((k1y + 2  k2y + 2  k3y + k4y)\), t0 = t0 + h, 
          x0 = newx, y0 = newy, AppendTo[solnslist2, {t0, x0, y0}]}\), ",", 
        RowBox[{"{", 
          StyleBox["5",
            FontColor->RGBColor[1, 0, 0]], "}"}]}], 
      "]"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"compare2", "=", 
        RowBox[{"Table", "[", 
          RowBox[{\({solnslist2[\([i]\)]}\), ",", 
            RowBox[{"{", 
              RowBox[{"i", ",", 
                StyleBox["1",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox[",",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox["6",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox[",",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox["1",
                  FontColor->RGBColor[1, 0, 0]]}], "}"}]}], "]"}]}], 
      ";"}], "\[IndentingNewLine]", \(fulllist2 = 
        Table[{exactsoln[\([i, 1]\)], exactsoln[\([i, 2]\)], 
            compare2[\([i, 1, 2]\)], exactsoln[\([i, 3]\)], 
            compare2[\([i, 1, 3]\)]}, {i, 1, 6}];\), "\[IndentingNewLine]", 
    RowBox[{\(TableForm[fulllist2, 
        TableHeadings \[Rule] {None, {"\<t\>", "\<x\>", "\<RungeKutta x\>", "\
\<y\>", "\<RungeKutta y\>"}}]\), "  "}]}], "Input"],

Cell["\<\
Note how close the Runge Kutta values are to the exact results. We can look \
at them with more precision if we wish.\
\>", "Text"],

Cell[BoxData[
    \(TableForm[SetPrecision[fulllist2, 10], 
      TableHeadings \[Rule] {None, {"\<t\>", "\<x\>", "\<RungeKutta x\>", \
"\<y\>", "\<RungeKutta y\>"}}]\)], "Input"]
}, Closed]],

Cell[CellGroupData[{

Cell["Applying the Runge-Kutta Method with h = 0.1", "Subsection"],

Cell[BoxData[{\(Clear[f, t, y, newx, newy, 
      solnslist3]\), "\[IndentingNewLine]", 
    RowBox[{\(f[x_, y_]\), ":=", 
      StyleBox[\(\[ExponentialE]\^\((\(-x\) + y)\) - Cos[x]\),
        FontColor->RGBColor[1, 0, 0]]}], "\[IndentingNewLine]", 
    RowBox[{\(g[x_, y_]\), ":=", 
      StyleBox[\(Sin[x - 3  y]\),
        FontColor->RGBColor[1, 0, 0]]}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"t0", "=", 
        StyleBox["0",
          FontColor->RGBColor[1, 0, 0]]}], ";"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"x0", "=", 
        StyleBox["1",
          FontColor->RGBColor[1, 0, 0]]}], ";"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"y0", "=", 
        StyleBox["2",
          FontColor->RGBColor[1, 0, 0]]}], ";"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"h", "=", 
        StyleBox[".1",
          FontColor->RGBColor[1, 0, 0]]}], 
      ";"}], "\[IndentingNewLine]", \(solnslist3 = {{t0, x0, 
            y0}};\), "\[IndentingNewLine]", 
    RowBox[{"Do", "[", 
      RowBox[{\({k1x = f[x0, y0]\ , k1y = g[x0, y0], 
          k2x = f[x0 + 1/2  h\ k1x, y0 + 1/2\ h\ k1y], 
          k2y = g[x0 + 1/2  h\ k1x, y0 + 1/2\ h\ k1y], 
          k3x = f[x0 + 1/2  h\ k2x, y0 + 1/2\ h\ k2y], 
          k3y = g[x0 + 1/2  h\ k2x, y0 + 1/2\ h\ k2y], 
          k4x = f[x0 + h\ k3x, y0 + h\ k3y], 
          k4y = g[x0 + h\ k3x, y0 + h\ k3y], 
          newx = x0 + h/6\ \((k1x + 2  k2x + 2  k3x + k4x)\), 
          newy = y0 + h/6\ \((k1y + 2  k2y + 2  k3y + k4y)\), t0 = t0 + h, 
          x0 = newx, y0 = newy, AppendTo[solnslist3, {t0, x0, y0}]}\), ",", 
        RowBox[{"{", 
          StyleBox["10",
            FontColor->RGBColor[1, 0, 0]], "}"}]}], 
      "]"}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"compare3", "=", 
        RowBox[{"Table", "[", 
          RowBox[{\({solnslist3[\([i]\)]}\), ",", 
            RowBox[{"{", 
              RowBox[{"i", ",", 
                StyleBox["1",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox[",",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox["11",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox[",",
                  FontColor->RGBColor[1, 0, 0]], 
                StyleBox["2",
                  FontColor->RGBColor[1, 0, 0]]}], "}"}]}], "]"}]}], 
      ";"}], "\[IndentingNewLine]", \(fulllist3 = 
        Table[{exactsoln[\([i, 1]\)], exactsoln[\([i, 2]\)], 
            compare3[\([i, 1, 2]\)], exactsoln[\([i, 3]\)], 
            compare3[\([i, 1, 3]\)]}, {i, 1, 
            6}];\), "\[IndentingNewLine]", \(TableForm[
      SetPrecision[fulllist3, 10], 
      TableHeadings \[Rule] {None, {"\<t\>", "\<x\>", "\<RungeKutta x\>", \
"\<y\>", "\<RungeKutta y\>"}}]\)}], "Input"],

Cell["These results are even better with the smaller value of h.", "Text"]
}, Closed]]
}, Open  ]]
}, Open  ]]
},
